{
  "name": "dashboard",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "deploy": "wrangler deploy",
    "dev": "remix dev --manual -c \"npm start\"",
    "start": "wrangler dev --port 3002 ./build/index.js",
    "migrations:gen": "drizzle-kit generate:sqlite",
    "local:migrations:apply": "wrangler d1 migrations apply frkt-db --local",
    "migrations:apply": "NO_D1_WARNING=true wrangler d1 migrations apply frkt-db",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "@remix-run/cloudflare": "^2.0.1",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "common": "workspace:*",
    "isbot": "^3.6.8",
    "jotai": "^2.4.3",
    "lucide-react": "^0.284.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-github": "^1.6.0",
    "remix-auth-google": "^1.2.0",
    "viem": "^1.16.2",
    "wagmi": "^1.4.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@types/node": "^20.4.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.6",
    "eslint": "^8.38.0",
    "postcss": "^8.4.31",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.1.6",
    "wrangler": "^3.1.1",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
